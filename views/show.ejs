<h1>Student Name</h1>

<h2>students classes</h2>


<button type="button" name="button">delete</button>


<h3> add courses to student</h3>


<ul >
  <% courses.forEach(function(val){ %>
  <li id='<%=val._id %>'><%= val.Name%> <%= val.Description%>
    <div>
    <button type="button" name="button" class="btn btn-primary">Edit</button>
    <button class="btn btn-primary delete" type="button" name="button" data-id='<%= val._id %>' data-type='/api/students/' >Delete</button>
    </div
  </li>
  <% }); %>
</ul>
